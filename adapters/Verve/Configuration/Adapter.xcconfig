//
//  Adapter.xcconfig
//  Adapter
//
/// Third Party AD Network Name.

AD_NETWORK_NAME = Verve
FRAMEWORK_NAME = $(AD_NETWORK_NAME)Adapter
/// Xcode variables
PRODUCT_NAME = VerveAdapter
EXECUTABLE_PREFIX =

/// User defined variables.
LIB_NAME = $(EXECUTABLE_PREFIX)$(PRODUCT_NAME)
OUTPUT_FOLDER = ${PROJECT_DIR}/Library
MIN_OS_MAJOR_VERSION = 12
MIN_OS_MINOR_VERSION = 0
MIN_OS_MAJOR_VERSION_64_BIT = 12
MIN_OS_MINOR_VERSION_64_BIT = 0

/// Xcode variables
/// General Build settings.
IPHONEOS_DEPLOYMENT_TARGET = $(MIN_OS_MAJOR_VERSION).$(MIN_OS_MINOR_VERSION)
IPHONEOS_DEPLOYMENT_TARGET[arch=arm64] = $(MIN_OS_MAJOR_VERSION_64_BIT).$(MIN_OS_MINOR_VERSION_64_BIT)
IPHONEOS_DEPLOYMENT_TARGET[arch=x86_64] = $(MIN_OS_MAJOR_VERSION_64_BIT).$(MIN_OS_MINOR_VERSION_64_BIT)
CURRENT_PROJECT_VERSION = 3710
MARKETING_VERSION = 3.7.1
FRAMEWORK_SEARCH_PATHS[sdk=iphoneos*] = $(inherited) "${SRCROOT}/Drop_Framework_And_Headers/GoogleMobileAds.xcframework/ios-arm64" "${SRCROOT}/Drop_Framework_And_Headers/HyBid.xcframework/ios-arm64" "${SRCROOT}/Drop_Framework_And_Headers/UserMessagingPlatform.xcframework/ios-arm64" "${SRCROOT}/Drop_Framework_And_Headers/iphoneos/GoogleMobileAds.xcframework/ios-arm64" "${SRCROOT}/Drop_Framework_And_Headers/iphoneos/HyBid.xcframework/ios-arm64" "${SRCROOT}/Drop_Framework_And_Headers/iphoneos/UserMessagingPlatform.xcframework/ios-arm64"
FRAMEWORK_SEARCH_PATHS[sdk=iphonesimulator*] = $(inherited) "${SRCROOT}/Drop_Framework_And_Headers/GoogleMobileAds.xcframework/ios-arm64_x86_64-simulator" "${SRCROOT}/Drop_Framework_And_Headers/HyBid.xcframework/ios-arm64_x86_64-simulator" "${SRCROOT}/Drop_Framework_And_Headers/UserMessagingPlatform.xcframework/ios-arm64_x86_64-simulator" "${SRCROOT}/Drop_Framework_And_Headers/iphonesimulator/GoogleMobileAds.xcframework/ios-arm64_x86_64-simulator" "${SRCROOT}/Drop_Framework_And_Headers/iphonesimulator/HyBid.xcframework/ios-arm64_x86_64-simulator" "${SRCROOT}/Drop_Framework_And_Headers/iphonesimulator/UserMessagingPlatform.xcframework/ios-arm64_x86_64-simulator"
LIBRARY_SEARCH_PATHS = $(inherited) "$(SRCROOT)/Drop_Framework_And_Headers/**"
HEADER_SEARCH_PATHS = $(inherited) "$(SRCROOT)/Drop_Framework_And_Headers/**"
SDKROOT = iphoneos
SUPPORTED_PLATFORMS = iphoneos iphonesimulator
ONLY_ACTIVE_ARCH = NO
VALID_ARCHS = arm64 x86_64
TARGETED_DEVICE_FAMILY = 1,2
BITCODE_GENERATION_MODE = bitcode
ENABLE_BITCODE = NO
CLANG_ENABLE_MODULES = YES
CLANG_ENABLE_OBJC_ARC = YES
GCC_WARN_UNDECLARED_SELECTOR = YES
CLANG_WARN_OBJC_IMPLICIT_RETAIN_SELF = YES
CLANG_WARN_OBJC_REPEATED_USE_OF_WEAK = YES
ALWAYS_SEARCH_USER_PATHS = NO
CLANG_WARN_OBJC_IMPLICIT_RETAIN_SELF = YES;
GCC_WARN_ABOUT_MISSING_PROTOTYPES = YES
GCC_WARN_UNUSED_VARIABLE = YES
GCC_WARN_ABOUT_RETURN_TYPE = YES
GCC_WARN_SHADOW = YES
PRESERVE_DEAD_CODE_INITS_AND_TERMS = YES
SEPARATE_STRIP = YES
STRIP_INSTALLED_PRODUCT = YES
DEAD_CODE_STRIPPING = YES
OTHER_CFLAGS = -fobjc-arc -fstack-protector-all
